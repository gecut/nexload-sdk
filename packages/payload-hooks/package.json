  "dependencies": {
    "@nexload-sdk/logger": "^3.0.0",
    "payload": "^3.64.0"
  },
  "devDependencies": {
    "@nexload-sdk/bundler": "workspace:*",
    "@nexload-sdk/eslint-config": "workspace:*",
    "@nexload-sdk/typescript-config": "workspace:*",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "typescript": "^5.4.5"
  }
