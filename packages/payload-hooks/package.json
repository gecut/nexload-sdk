{
  "name": "@nexload-sdk/payload-hooks",
  "version": "1.0.2",
  "private": false,
  "description": "Effortless, type-safe logging for Payload CMS collection operations.",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "keywords": [
    "payload",
    "payload-cms",
    "hooks",
    "logging",
    "nexload",
    "sdk"
