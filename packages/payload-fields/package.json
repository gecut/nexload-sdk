{
  "name": "@nexload-sdk/payload-fields",
  "version": "0.1.0",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./fields/date": {
      "types": "./dist/fields/date/index.d.ts",
      "import": "./dist/fields/date/index.mjs",
      "require": "./dist/fields/date/index.cjs"
    },
    "./fields/date/date-picker": {
      "types": "./dist/fields/date/date-picker.d.ts",
      "import": "./dist/fields/date/date-picker.mjs",
      "require": "./dist/fields/date/date-picker.cjs"
    },
    "./fields/date/date-cell": {
      "types": "./dist/fields/date/date-cell.d.ts",
      "import": "./dist/fields/date/date-cell.mjs",
      "require": "./dist/fields/date/date-cell.cjs"
    },
    "./fields/slug": {
      "types": "./dist/fields/slug/index.d.ts",
      "import": "./dist/fields/slug/index.mjs",
      "require": "./dist/fields/slug/index.cjs"
    },
    "./fields/slug/slug-field": {
      "types": "./dist/fields/slug/slug-field.d.ts",
      "import": "./dist/fields/slug/slug-field.mjs",
      "require": "./dist/fields/slug/slug-field.cjs"
    }
  },
  "scripts": {
    "build": "node esbuild.config.mjs",
    "dev": "node ../../tools/bundler/index.js --watch",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    "@payloadcms/richtext-lexical": "^3.64.0",
    "@payloadcms/ui": "^3.64.0",
    "date-fns-jalali": "4.1.0-0",
    "react-day-picker": "^9.11.1"
  },
  "peerDependencies": {
    "payload": "^2.0.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@nexload-sdk/eslint-config": "workspace:*",
    "@nexload-sdk/typescript-config": "workspace:*",
    "@types/react": "^18.2.0",
    "typescript": "^5.4.5",
    "eslint": "^8.57.0",
    "payload": "^2.11.0",
    "react": "^18.2.0"
  }
}